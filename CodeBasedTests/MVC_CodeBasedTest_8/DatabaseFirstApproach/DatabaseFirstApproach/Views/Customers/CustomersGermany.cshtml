@model IEnumerable<DatabaseFirstApproach.Models.Customer>
@{
    ViewBag.Title = "CustomersGermany";
}

<h2>Customers in Germany</h2>

@using (Html.BeginForm())
{
        <table border="1" cellspacing="5" cellpadding="10" align="center">
            <tr>
                <th>Customer ID of customers in the Germany</th>
               
            </tr>
            @foreach (var c in Model)
            {
                <tr>
                    <td>@c.CustomerID</td>
                    
                </tr>
            }
        </table>
}


